{
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules":{
        "semi":["error","always"]
    },
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    }
}